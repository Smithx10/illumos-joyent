$(ROOT_BCM2835_KERN_DIR)/%: $(OBJS_DIR)/% $(ROOT_BCM2835_KERN_DIR) FRC
	$(INS.file)

$(ROOT_BCM2835_DRV_DIR)/%: $(OBJS_DIR)/% $(ROOT_BCM2835_DRV_DIR) FRC
	$(INS.file)

$(ROOT_BCM2835_CPU_DIR)/%: $(OBJS_DIR)/% $(ROOT_BCM2835_CPU_DIR) FRC
	$(INS.file)

$(ROOT_BCM2835_MISC_DIR)/%: $(OBJS_DIR)/% $(ROOT_BCM2835_MISC_DIR) FRC
	$(INS.file)

#
# Include common targets
#
include $(UTSBASE)/armv6/bcm2835/Makefile.rules
include $(UTSBASE)/armv6/Makefile.targ
