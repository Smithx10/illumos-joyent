.KEEP_STATE:

$(ROOT_BCM2835_DIR): $(ROOT_PLAT_DIR)
	-$(INS.dir)

$(ROOT_BCM2835_MOD_DIR): $(ROOT_BCM2835_DIR)
	-$(INS.dir)

$(ROOT_BCM2835_CPU_DIR_32): $(ROOT_BCM2835_MOD_DIR)
	-$(INS.dir)

$(ROOT_BCM2835_CPU_DIR_64): $(ROOT_BCM2835_CPU_DIR_32)
	-$(INS.dir)

$(ROOT_BCM2835_DRV_DIR_32): $(ROOT_BCM2835_MOD_DIR)
	-$(INS.dir)

$(ROOT_BCM2835_DRV_DIR_64): $(ROOT_BCM2835_DRV_DIR_32)
	-$(INS.dir)

$(ROOT_BCM2835_MISC_DIR_32): $(ROOT_BCM2835_MOD_DIR)
	-$(INS.dir)

$(ROOT_BCM2835_MISC_DIR_64): $(ROOT_BCM2835_MISC_DIR_32)
	-$(INS.dir)

$(ROOT_BCM2835_CRYPTO_DIR_32): $(ROOT_BCM2835_MOD_DIR)
	-$(INS.dir)

$(ROOT_BCM2835_CRYPTO_DIR_64): $(ROOT_BCM2835_CRYPTO_DIR_32)
	-$(INS.dir)

$(USR_BCM2835_DIR): $(USR_PLAT_DIR)
	-$(INS.dir)

$(USR_BCM2835_INC_DIR): $(USR_BCM2835_DIR)
	$(INS.slink4)

$(USR_BCM2835_SBIN_DIR): $(USR_BCM2835_DIR)
	-$(INS.slink5)

$(USR_BCM2835_LIB_DIR): $(USR_BCM2835_DIR)
	-$(INS.dir)

$(USR_BCM2835_ISYS_DIR): $(USR_BCM2835_INC_DIR)
